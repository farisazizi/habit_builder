
<%= render 'habits/error_message', object: f.object %>

<%= f.label :habit_category, "Category" %>
<% collection = [    ['Self-improvement', 'Self-improvement'],
                     ['Fitness', 'Fitness'],
                     ['Reading', 'Reading'],
                     ['Social', 'Social'],
                     ['Study', 'Study'],
                     ['Work', 'Work'],
                     ['Arts', 'Arts'],
                     ['Finance', 'Finance'],
                     ['Others', 'Others']
                ]%>
<%= f.select :habit_category, collection %>
<br><br>

<%= f.label :habit_title, "Title" %>
<%= f.text_field :habit_title, class: 'form-control' %>

<br>

<%= f.label :habit_description, "Description" %>
<%= f.text_field :habit_description, class: 'form-control' %>

<br>

<%= f.label :habit_duration, "Duration" %>
<% collectionD = [    ['21','21']   ]%>
<%= f.select :habit_duration, collectionD %>


