
<div class="page-header">
  <h1>Index page</h1>
</div>


<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tracks.each do |track| %>
      <div class="row ">
    <aside class="col-md-4">
        <p>The Habit Starts Today</p>
        <p>The Habit is at</p>
        <h1><%= track.streak %> days</h1>
        <div class="progress">
          <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;">
            <%= track.streak %> days
          </div>
        </div>
        <p>Completion of the Habit</p>
    </aside>
</div>
    <% end %>
  </tbody>
</table>
