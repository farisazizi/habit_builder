<h1>Listing Habits</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @habits.each do |habit| %>
      <tr>
        <td> <%= habit.habit_title %> </td>
        <td> Longest streak is <%= habit.track.streak %> </td>
        <td> Habit completed at  </td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>